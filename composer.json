{
    "name": "tomatophp/filament-splade",
    "type": "library",
    "description": "Integration of Splade Vue Components for Filament",
    "keywords": [
        "php",
        "laravel",
        "splade",
        "filament",
        "vuejs3",
        "vue3",
        "components",
        "form",
        "table",
        "modal",
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\FilamentSplade\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\FilamentSplade\\FilamentSpladeServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "tomatophp/console-helpers": "^1.1",
        "filament/filament": "^3.0.0",
        "protonemedia/laravel-splade": "^1.4"
    }
}
